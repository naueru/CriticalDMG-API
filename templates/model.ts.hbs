import { ModelName } from "../declarations";
import {
  Model,
  Column,
  Table,
} from "sequelize-typescript";

@Table({
  tableName: ModelName.{{constantCase name}},
  modelName: ModelName.{{constantCase name}},
  timestamps: true,
})
export class {{properCase name}}Model extends Model<{{properCase name}}> {
{{#each properties}}
  @Column
  {{camelCase this.propertyName}}!: {{this.propertyType}};
{{/each}}
  

  @BeforeCount
  static setToRaw(options: any) {
    options.raw = true;
  }
}

export default {{properCase name}}Model;
